import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int count  = 0;
		if(x>=7){
		    System.out.println(3);
		}else if(x>=3 && (x<=6)){
		    System.out.println(2);
		}else if(x<=2 && (x!=0)){
		    System.out.println(1);
		}else{
		    System.out.println(0);
		}

	}
}

