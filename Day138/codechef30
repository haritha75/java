import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int n = sc.nextInt();
		int ladduRate = n*10;
		int withoutLaddu = x-ladduRate;
		System.out.println(withoutLaddu/20);

	}
}

