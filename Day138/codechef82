import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i=0;i<t;i++){
		    int h = sc.nextInt();
		    int l = sc.nextInt();
		    int w = sc.nextInt();
		    int a = 2*((h*l)+(l*w)+(w*h));
		    System.out.println((1000/a));
		}

	}
}

