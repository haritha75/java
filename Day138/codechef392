import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
	    int x=sc.nextInt();
	    if(x>=200){
	        System.out.println(x-50);
	    }else{
	    System.out.println(x);
		}// your code goes here

	}
}

