import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int x = s.nextInt();
        int y = s.nextInt();
        
        if(2*x<=n && 2*y <= n){
            System.out.println("Yes");
        }
        else{
            System.out.println("No");
        }


	}
}
