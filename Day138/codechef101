import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();
		if(x>=y){
		    System.out.println(0);
		}else{
		    if(y%2==0){
		        System.out.println((y/x)-1);
		    }
		    else{
		        System.out.println((y/x));

		    }
		}

	}
}

