import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i=0;i<t;i++) {
		    int n = sc.nextInt();
		    if(n==1)
		    System.out.println(1);
		    else if(n<4)
		    System.out.println(n-1);
		    else
		    System.out.println(n);
		}

	}
}

