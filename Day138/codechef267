import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int f1 = sc.nextInt();
		int p1 = sc.nextInt();
		int f2 = sc.nextInt();
		int p2 = sc.nextInt();
		int to = Math.abs(f1-p1);
		int to1 = Math.abs(f2-p2);
		if(to==to1){
		    System.out.println("Both");
		}else{
		    System.out.println((to>to1)?"Second":"First");
		}

	}
}

