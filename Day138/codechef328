import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int s = sc.nextInt();

        int nP = 5000;
        int sP = 9800;

        int Earnings = n * nP + s * sP;

        System.out.println(Earnings);
	}
}

