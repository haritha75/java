import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int a=s.nextInt();
        int b=s.nextInt();
        int ic=n-a*b;
        System.out.println(ic);
	
	}
}

