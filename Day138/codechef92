import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int j = sc.nextInt(),s=sc.nextInt(),m=sc.nextInt();
		int l = m-j;
		System.out.println(((l/s)%2==0)?"Lucky Chef":"Unlucky Chef");


	}
}

