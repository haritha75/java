import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
			int m=sc.nextInt();
			int a=2*n-m;
			if(2*n<=m) System.out.println(n);
			else System.out.println(n-a);
		 
		

	}
}

