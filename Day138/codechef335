import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s=new Scanner(System.in);
		int t=s.nextInt();
	
		for(int i=0;i<t;i++){
		    int n=s.nextInt();
		   
		    int a=n/2;
		    int l=a/2;
		    int b=a-l;
		    System.out.println(l*b);
		        
		        
		}

	}
}

